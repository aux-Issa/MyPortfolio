<%= form_for(@article) do |f|%>
  <!---%= render'shared/error_messages', object: f.object %>--->
  <div class="field">
    <%= f.text_area :title, placeholder:"タイトル" %>
  </div>  
  <div class="field">
    <%= f.text_area :content, placeholder:"記事" %>
  </div>
  <%= f.submit"Post", class: "btn btn-primary" %>
<% end %>